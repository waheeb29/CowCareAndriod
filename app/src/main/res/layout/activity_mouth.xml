<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mouth">

    <TextView
        android:id="@+id/txtsuggestion"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="@color/colorPrimaryDark"
        android:textColor="#FA422020"
        android:textColorHint="#FA000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="239dp"
        android:layout_height="40dp"
        android:text="Profuse Salivation"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/Black"
        android:textColorLink="#00000000"
        android:textSize="30sp"
        app:fontFamily="cursive"
        app:layout_constraintBottom_toBottomOf="@+id/txtsuggestion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtsuggestion" />

    <TextView
        android:id="@+id/txteyes"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="408dp"
        android:gravity="center"
        android:text="Select Eyes Color"
        android:textColor="@color/Black"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtsuggestion"
        app:layout_constraintVertical_bias="0.235" />

    <Spinner
        android:id="@+id/Spinner_eyes"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginTop="2dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txteyes"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txteyes"
        app:layout_constraintTop_toBottomOf="@+id/txteyes"
        app:layout_constraintVertical_bias="0.029999971" />

    <TextView
        android:id="@+id/txtsalivation"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="268dp"
        android:gravity="center"
        android:text="Select Salivation Amount"
        android:textColor="@color/Black"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Spinner_eyes"
        app:layout_constraintVertical_bias="0.171" />

    <Spinner
        android:id="@+id/Spinner_salivation"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginTop="2dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txteyes"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txteyes"
        app:layout_constraintTop_toBottomOf="@+id/txtsalivation"
        app:layout_constraintVertical_bias="0.029999971" />

    <TextView
        android:id="@+id/txtfeet"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="188dp"
        android:gravity="center"
        android:text="Any Wound on Feet"
        android:textColor="@color/Black"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Spinner_salivation"
        app:layout_constraintVertical_bias="0.244" />

    <Spinner
        android:id="@+id/Spinner_feet"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginTop="2dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txteyes"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/txteyes"
        app:layout_constraintTop_toBottomOf="@+id/txtfeet"
        app:layout_constraintVertical_bias="0.029999971" />

    <TextView
        android:id="@+id/txtulcer"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="84dp"
        android:gravity="center"
        android:text="Any wound inside mouth"
        android:textColor="@color/Black"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Spinner_feet"
        app:layout_constraintVertical_bias="0.25" />

    <Spinner
        android:id="@+id/Spinner_ulcer"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:layout_marginBottom="28dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtulcer"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txteyes"
        app:layout_constraintTop_toBottomOf="@+id/txtulcer"
        app:layout_constraintVertical_bias="0.102" />

    <Button
        android:id="@+id/btnproceed"
        android:layout_width="340dp"
        android:layout_height="35dp"
        android:background="@drawable/selector"
        android:text="Proceed"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Spinner_ulcer"
        app:layout_constraintStart_toStartOf="@+id/Spinner_ulcer"
        app:layout_constraintTop_toBottomOf="@+id/Spinner_ulcer" />


</androidx.constraintlayout.widget.ConstraintLayout>